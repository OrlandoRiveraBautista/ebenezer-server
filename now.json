{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/videos",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/latestvideo",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/live",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/posts/store",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/posts",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/give",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/success",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/signup",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/login",
      "dest": "/index.js",
      "methods": ["POST"]
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "ORLANDOPASSWORD": "@orlandopassword",
    "AWSAccessKeyId": "@awsaccesskeyid",
    "AWSSecretKey": "@awssecretkey",
    "S3BucketName": "@s3bucketname",
    "PAYPALClientID": "@paypalclientid",
    "PAYPALClientSecret": "@paypalclientsecret",
    "JWT_KEY": "@jwt_key",
    "FRONT_URL": "https://ebenezer-virtual.herokuapp.com/"
  }
}
